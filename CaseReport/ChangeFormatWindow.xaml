<Window x:Class="CaseReport.ChangeFormatWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="修改Excel文件格式" Height="500" Width="500">
    <Grid>
        <TextBox x:Name="columnname_label" HorizontalAlignment="Left" Height="138" Margin="25,80,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="354" TextChanged="TextBox_TextChanged_1"/>
        <Label  Content="病理名 " HorizontalAlignment="Left" VerticalAlignment="Top" Margin="25,23,0,0"/>
        <Button x:Name ="btn_confirm" Content="确认" HorizontalAlignment="Left" Margin="36,240,0,0" VerticalAlignment="Top" Width="75" Click="btn_confirm_Click"/>
        <Label  Content="列名 " HorizontalAlignment="Left" Margin="25,54,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btn_cancel" Content="取消" HorizontalAlignment="Left" Margin="287,240,0,0" VerticalAlignment="Top" Width="75" Click="btn_cancel_Click"/>
        <TextBox x:Name="casename_label" HorizontalAlignment="Left" Height="23" Margin="79,26,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="76"/>
        <Button x:Name="btn_delete" Content="删除" HorizontalAlignment="Left" Margin="159,240,0,0" VerticalAlignment="Top" Width="75" Click="btn_delete_Click"/>

    </Grid>
</Window>
